{
  "name": "streemian-addposting",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browser": {
    "ws": false
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir lib",
    "build:watch": "babel --watch src --presets babel-preset-es2015 --out-dir lib & cp src/index.html dist/index.html",
    "postbuild": "cp src/index.html dist/index.html",
    "prebrowserify": "npm run build",
    "browserify": "browserify lib/index.js --standalone bundle -o dist/bundle.js -d",
    "postbrowserify": "uglifyjs --compress --mangle --sequences --drop_console --output dist/bundle.min.js -- dist/bundle.js"
  },
  "author": "Sigve Kvalsvik <bitsharesblocks@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.1.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "steemjs-lib": "^0.2.14"
  }
}
